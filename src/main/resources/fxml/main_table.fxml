<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.CustomMenuItem?>
<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuButton?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.control.SeparatorMenuItem?>
<?import javafx.scene.control.Slider?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>

<BorderPane maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" minHeight="0.0" minWidth="-Infinity" prefWidth="600.0" xmlns="http://javafx.com/javafx/" xmlns:fx="http://javafx.com/fxml/1" fx:controller="seng202.team3.gui.TableController">
   <center>
      <TableView fx:id="mainTable" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" minHeight="0.0" onMouseClicked="#setCharger" BorderPane.alignment="CENTER" />
   </center>
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
   <top>
      <HBox alignment="CENTER_LEFT" BorderPane.alignment="CENTER_LEFT">
         <children>
            <TextField fx:id="searchCharger" prefHeight="40" promptText="Search Charger" style="-fx-font-size: 20;" />
            <MenuButton fx:id="filters" minHeight="30.0" mnemonicParsing="false" prefWidth="250.0" style="-fx-font-size: 20;" text="Filter">
               <items>
                  <SeparatorMenuItem mnemonicParsing="false" />
                  <CustomMenuItem hideOnClick="false" mnemonicParsing="false" text="Unspecified Action">
                     <content>
                        <CheckBox fx:id="toggleTimeLimit" style="-fx-text-fill: black;" />
                     </content>
                  </CustomMenuItem>
                  <CustomMenuItem hideOnClick="false" mnemonicParsing="false" text="Unspecified Action">
                     <content>
                        <Slider fx:id="timeLimit" majorTickUnit="30.0" max="520.0" min="30.0" minorTickCount="10" showTickLabels="true" showTickMarks="true" style="-fx-text-fill: black;" value="60.0" />
                     </content>
                  </CustomMenuItem>
                  <SeparatorMenuItem mnemonicParsing="false" />
                  <CustomMenuItem hideOnClick="false" mnemonicParsing="false" text="Unspecified Action">
                     <content>
                        <CheckBox fx:id="onParkingFilter" style="-fx-text-fill: black;" />
                     </content>
                  </CustomMenuItem>
                  <CustomMenuItem hideOnClick="false" mnemonicParsing="false" text="Unspecified Action">
                     <content>
                        <Slider fx:id="parkingLot" blockIncrement="5.0" majorTickUnit="5.0" max="20.0" min="1.0" minorTickCount="1" showTickLabels="true" showTickMarks="true" snapToTicks="true" style="-fx-: black;" value="5.0" />
                     </content>
                  </CustomMenuItem>
                  <Menu fx:id="cpMenu" mnemonicParsing="false" text="Carpark Cost">
                     <items>
                        <CustomMenuItem hideOnClick="false" mnemonicParsing="false">
                           <content>
                              <CheckBox fx:id="withoutCarparkCost" style="-fx-text-fill: black;" text="Without Cost" />
                           </content>
                        </CustomMenuItem>
                        <CustomMenuItem hideOnClick="false" mnemonicParsing="false" text="Unspecified Action">
                           <content>
                              <CheckBox fx:id="withCarparkCost" style="-fx-text-fill: black;" text="With Cost" />
                           </content>
                        </CustomMenuItem>
                     </items>
                  </Menu>
                  <SeparatorMenuItem mnemonicParsing="false" />
                  <Menu fx:id="chargerTypes" mnemonicParsing="false" text="Charger Types">
                     <items>
                        <CustomMenuItem hideOnClick="false" mnemonicParsing="false" text="AC button">
                           <content>
                              <CheckBox fx:id="acButton" style="-fx-text-fill: black;" text="AC charging port" textFill="black" />
                           </content>
                        </CustomMenuItem>
                        <CustomMenuItem hideOnClick="false" mnemonicParsing="false" text="DC button">
                           <content>
                              <CheckBox fx:id="dcButton" style="-fx-text-fill: black;" text="DC charging port" textFill="black" />
                           </content>
                        </CustomMenuItem>
                     </items>
                  </Menu>
                  <SeparatorMenuItem mnemonicParsing="false" />
                  <Menu fx:id="openMenu" mnemonicParsing="false" text="Hours Open">
                     <items>
                        <CustomMenuItem hideOnClick="false" mnemonicParsing="false" text="Unspecified Action">
                           <content>
                              <CheckBox fx:id="openAllButton" style="-fx-text-fill: black;" text="Open 24 hours" />
                           </content>
                        </CustomMenuItem>
                        <CustomMenuItem hideOnClick="false" mnemonicParsing="false" text="Unspecified Action">
                           <content>
                              <CheckBox fx:id="notOpenAllButton" style="-fx-text-fill: black;" text="Not Open 24 Hours" />
                           </content>
                        </CustomMenuItem>
                     </items>
                  </Menu>
                  <SeparatorMenuItem mnemonicParsing="false" />
                  <Menu fx:id="attractMenu" mnemonicParsing="false" text="Nearby Attractions">
                     <items>
                        <CustomMenuItem hideOnClick="false" mnemonicParsing="false" text="Has Attraction button">
                           <content>
                              <CheckBox fx:id="attractionButton" style="-fx-text-fill: black;" text="Nearby Attraction" />
                           </content>
                        </CustomMenuItem>
                        <CustomMenuItem hideOnClick="false" mnemonicParsing="false">
                           <content>
                              <CheckBox fx:id="noNearbyAttraction" style="-fx-text-fill: black;" text="No Nearby Attraction" />
                           </content>
                        </CustomMenuItem>
                     </items>
                  </Menu>
                  <SeparatorMenuItem mnemonicParsing="false" />
                  <Menu fx:id="cCostsMenu" mnemonicParsing="false" text="Charging Cost">
                     <items>
                        <CustomMenuItem hideOnClick="false" mnemonicParsing="false" text="No Charging Cost">
                           <content>
                              <CheckBox fx:id="chargingCost" style="-fx-text-fill: black;" text="No Charging Cost" />
                           </content>
                        </CustomMenuItem>
                        <CustomMenuItem hideOnClick="false" mnemonicParsing="false" text="Unspecified Action">
                           <content>
                              <CheckBox fx:id="hasChargingCost" style="-fx-text-fill: black;" text="Charing Cost" />
                           </content>
                        </CustomMenuItem>
                     </items>
                  </Menu>
               </items>
            </MenuButton>
            <MenuButton fx:id="columnEdit" minHeight="30.0" mnemonicParsing="false" prefWidth="250.0" style="-fx-font-size: 20;" text="Select Columns">
               <items>
                  <CustomMenuItem hideOnClick="false" mnemonicParsing="false" text="Unspecified Action">
                    <content>
                      <CheckBox fx:id="showId" style="-fx-text-fill: black;" text="Show ID" />
                    </content>
                  </CustomMenuItem>
                  <CustomMenuItem hideOnClick="false" mnemonicParsing="false" text="Unspecified Action">
                     <content>
                        <CheckBox fx:id="showOperator" selected="true" style="-fx-text-fill: black;" text="Show Operator" />
                     </content>
                  </CustomMenuItem>
                  <CustomMenuItem hideOnClick="false" mnemonicParsing="false" text="Unspecified Action">
                     <content>
                        <CheckBox fx:id="showAddress" selected="true" style="-fx-text-fill: black;" text="Show Address" />
                     </content>
                  </CustomMenuItem>
                  <CustomMenuItem hideOnClick="false" mnemonicParsing="false" text="Unspecified Action">
                     <content>
                        <CheckBox fx:id="showOwner" mnemonicParsing="false" selected="true" text="Show Owner" />
                     </content>
                  </CustomMenuItem>
                  <CustomMenuItem hideOnClick="false" mnemonicParsing="false" text="Unspecified Action">
                     <content>
                        <CheckBox fx:id="showHoursOpen" selected="true" style="-fx-text-fill: black;" text="Show If 24/7" />
                     </content>
                  </CustomMenuItem>
                  <CustomMenuItem hideOnClick="false" mnemonicParsing="false" text="Unspecified Action">
                     <content>
                        <CheckBox fx:id="showCarparks" selected="true" style="-fx-text-fill: black;" text="Show Carparks" />
                     </content>
                  </CustomMenuItem>
                  <CustomMenuItem hideOnClick="false" mnemonicParsing="false" text="Unspecified Action">
                     <content>
                        <CheckBox fx:id="showCarparkCost" selected="true" style="-fx-text-fill: black;" text="Show Carpark Cost" />
                     </content>
                  </CustomMenuItem>
                  <CustomMenuItem hideOnClick="false" mnemonicParsing="false" text="Unspecified Action">
                     <content>
                        <CheckBox fx:id="showTimeLimit" selected="true" style="-fx-text-fill: black;" text="Show Time Limit" />
                     </content>
                  </CustomMenuItem>
                  <CustomMenuItem hideOnClick="false" mnemonicParsing="false" text="Unspecified Action">
                     <content>
                        <CheckBox fx:id="showAttraction" selected="true" style="-fx-text-fill: black;" text="Show Has Attractions" />
                     </content>
                  </CustomMenuItem>
                  <CustomMenuItem hideOnClick="false" mnemonicParsing="false" text="Unspecified Action">
                     <content>
                        <CheckBox fx:id="showLat" selected="true" style="-fx-text-fill: black;" text="Show LatPos" />
                     </content>
                  </CustomMenuItem>
                  <CustomMenuItem hideOnClick="false" mnemonicParsing="false" text="Unspecified Action">
                     <content>
                        <CheckBox fx:id="showLon" selected="true" style="-fx-text-fill: black;" text="Show LonPos" />
                     </content>
                  </CustomMenuItem>
                  <CustomMenuItem hideOnClick="false" mnemonicParsing="false" text="Unspecified Action">
                     <content>
                        <CheckBox fx:id="showOpening" selected="true" style="-fx-text-fill: black;" text="Show Date Opened" />
                     </content>
                  </CustomMenuItem>
                  <CustomMenuItem hideOnClick="false" mnemonicParsing="false" text="Unspecified Action">
                     <content>
                        <CheckBox fx:id="showChargingCost" selected="true" style="-fx-text-fill: black;" text="Show Charging Cost" />
                     </content>
                  </CustomMenuItem>
                  <CustomMenuItem hideOnClick="false" mnemonicParsing="false" text="Unspecified Action">
                     <content>
                        <CheckBox fx:id="showCurrent" selected="true" style="-fx-text-fill: black;" text="Show Current Type" />
                     </content>
                  </CustomMenuItem>
               </items>
            </MenuButton>
            <MenuButton minHeight="30.0" mnemonicParsing="false" prefWidth="250.0" style="-fx-font-size: 20;" text="Modify Chargers" fx:id="modChargers">
               <items>
                  <MenuItem mnemonicParsing="false" onAction="#addCharger" text="Add Charger" fx:id="addCharger"/>
                  <MenuItem mnemonicParsing="false" onAction="#editCharger" text="Edit Charger" fx:id="editCharger"/>
                  <MenuItem mnemonicParsing="false" onAction="#deleteCharger" text="Delete Charger" fx:id="deleteCharger"/>
               </items>
            </MenuButton>
            <Button fx:id="update" minHeight="30.0" mnemonicParsing="false" onAction="#updateTable" prefWidth="240.0" style="-fx-font-size: 20;" text="Update table" />
         </children>
         <padding>
            <Insets bottom="10.0" right="10.0" top="10.0" />
         </padding>
      </HBox>
   </top>
</BorderPane>
